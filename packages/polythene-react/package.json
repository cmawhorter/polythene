{
  "name": "polythene-react",
  "version": "1.5.4",
  "description": "",
  "main": "dist/polythene-react",
  "module": "dist/polythene-react.mjs",
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf dist/*",
    "rollup": "../../node_modules/rollup/bin/rollup -c ../../scripts/bundling/rollup.umd.js && ../../node_modules/rollup/bin/rollup -c ../../scripts/bundling/rollup.es.js",
    "build:standalone": "ENTRY=scripts/standalone.js DEST=./dist/polythene-react-standalone ../../node_modules/rollup/bin/rollup -c ../../scripts/bundling/rollup.browser.js",
    "build": "npm run clean && npm run rollup",
    "typecheck": "../../node_modules/typescript/bin/tsc --noEmit --strict --allowJs --checkJs --noImplicitAny false --lib es2015,DOM,ScriptHost --esModuleInterop true src/index.js"
  },
  "files": [
    "dist",
    "index.d.ts",
    "README.md"
  ],
  "dependencies": {
    "polythene-core-css": "1.5.4",
    "polythene-core": "1.5.4",
    "polythene-css": "1.5.4",
    "polythene-react-base": "1.5.4",
    "polythene-react-button-group": "1.5.4",
    "polythene-react-button": "1.5.4",
    "polythene-react-card": "1.5.4",
    "polythene-react-checkbox": "1.5.4",
    "polythene-react-dialog-pane": "1.5.4",
    "polythene-react-dialog": "1.5.4",
    "polythene-react-drawer": "1.5.4",
    "polythene-react-fab": "1.5.4",
    "polythene-react-icon-button": "1.5.4",
    "polythene-react-icon": "1.5.4",
    "polythene-react-ios-spinner": "1.5.4",
    "polythene-react-list-tile": "1.5.4",
    "polythene-react-list": "1.5.4",
    "polythene-react-material-design-progress-spinner": "1.5.4",
    "polythene-react-material-design-spinner": "1.5.4",
    "polythene-react-menu": "1.5.4",
    "polythene-react-notification": "1.5.4",
    "polythene-react-radio-button": "1.5.4",
    "polythene-react-radio-group": "1.5.4",
    "polythene-react-raised-button": "1.5.4",
    "polythene-react-ripple": "1.5.4",
    "polythene-react-search": "1.5.4",
    "polythene-react-shadow": "1.5.4",
    "polythene-react-slider": "1.5.4",
    "polythene-react-snackbar": "1.5.4",
    "polythene-react-svg": "1.5.4",
    "polythene-react-switch": "1.5.4",
    "polythene-react-tabs": "1.5.4",
    "polythene-react-textfield": "1.5.4",
    "polythene-react-toolbar": "1.5.4"
  },
  "peerDependencies": {
    "react": "^16.2.0",
    "react-hyperscript": "^3.0.0"
  },
  "devDependencies": {
    "@types/react": "^16.7.20"
  },
  "author": "Arthur Clemens <arthurclemens@gmail.com> (http://arthurclemens.com)",
  "homepage": "https://github.com/ArthurClemens/polythene",
  "license": "MIT"
}
